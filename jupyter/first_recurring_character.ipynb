   "source": [
    "def first_recurring_character(string): # time O(n), space O(n)\n",
    "    seen = set()\n",
    "    for c in string:\n",
    "        if c in seen:\n",
    "            return c\n",
    "        seen.add(c)\n",
    "\n",
    "    return None\n",
    "\n",
    "\n",
    "print(first_recurring_character('qwertty'))\n",
    "# t\n",
    "print(first_recurring_character('qwerty'))\n",
    "# None"
   ]
  },
